{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:6002",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "WeRead",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "windows": [],
    "security": {
      "csp": null,
      "dangerousRemoteDomainIpcAccess": [
        {
          "domain": "weread.qq.com",
          "windows": ["weread"],
          "plugins": ["store"],
          "enableTauriAPI": true
        }
      ]
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.qq.weread",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "systemTray": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true
    }
  }
}
